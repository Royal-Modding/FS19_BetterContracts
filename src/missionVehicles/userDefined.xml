            <vehicle filename="$data/vehicles/jcb/fastrac8330/fastrac8330.xml">
                <!-- tractor -->
                <configuration name="wheel" id="4" />
            </vehicle>
            <vehicle filename="$data/vehicles/jcb/fastrac8330/fastrac8330.xml">
                <!-- tractor -->
                <configuration name="wheel" id="4" />
            </vehicle>
            <vehicle filename="$data/vehicles/kroeger/TAW30/TAW30.xml" /> <!-- trailer -->
            <vehicle filename="$data/vehicles/kroeger/TAW30/TAW30.xml" /> <!-- trailer2 -->
        </group>

        <!-- potatoMission -->
