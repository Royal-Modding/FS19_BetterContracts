<?xml version="1.0" encoding="UTF-8"?>
<missionVehicles overwrite="false">
    <mission type="harvest">
        <!-- grainMission -->
        <group fieldSize="large" variant="GRAIN">
            <vehicle filename="vehicles/claas/dominator108SL/dominator108SL.xml" requiredMod="FS19_claasDominator108SL"> <!-- combine -->
                <configuration name="buyableGPS" id="2" requiredMod="FS19_guidanceSteering" />
                <configuration name="universalPassenger" id="1" requiredMod="FS19_UniversalPassenger"/>
            </vehicle>
            <vehicle filename="vehicles/claas/c510/c510.xml" requiredMod="FS19_claasDominator108SL"/> <!-- header -->
            <vehicle filename="vehicles/claas/c510Trailer/c510Trailer.xml" requiredMod="FS19_claasDominator108SL"/> <!-- headerTrailer -->
            <vehicle filename="vehicles/claas/arion400/arion400.xml" requiredMod="pdlc_claasPack"> 
                <!-- tractor (mod configs not loaded -like tireSound- will be ignored) -->
                <configuration name="motor" id="4" />
                <configuration name="wheel" id="6" />
                <configuration name="tireSound" id="6"/>
                <configuration name="soundExtension" id="2" requiredMod="FS19_changedMotorSounds"/>
                <configuration name="buyableGPS" id="2" requiredMod="FS19_guidanceSteering" />
                <configuration name="universalPassenger" id="1" requiredMod="FS19_UniversalPassenger"/>
            </vehicle>
            <vehicle filename="$data/vehicles/strautmann/SEK802/SEK802.xml" > 
                <!-- trailer -->
                <configuration name="fillVolume" id="4" />
                <configuration name="trailer" id="3" />
                <configuration name="fillUnit" id="4" />
            </vehicle>
        </group>

        <!-- maizeMission -->
        <group fieldSize="large" variant="MAIZE">
            <vehicle filename="$data/vehicles/johnDeere/S790/S790.xml">
                <!-- combine -->
                <configuration name="wheel" id="4" />
            </vehicle>
            <vehicle filename="$data/vehicles/capello/quasarHS16/quasarHS16.xml">
                <!-- header -->
                <configuration name="baseMaterial" id="23" />
            </vehicle>
            <vehicle filename="$data/vehicles/thueringerAgrar/leguan40/leguan40.xml" /> <!-- headerTrailer -->
            <vehicle filename="$data/vehicles/newHolland/T7/T7.xml">
                <!-- tractor -->
                <configuration name="motor" id="2" />
                <configuration name="wheel" id="4" />
            </vehicle>
            <vehicle filename="$data/vehicles/joskin/drakkar8600/drakkar8600.xml" /> <!-- trailer -->
        </group>

        <!-- sugarBeetMission -->
